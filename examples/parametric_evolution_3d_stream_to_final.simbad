final_snapshot
{  
  final_estimator
  {
    class population_size
    parameters
    {
      start_size 100000
    }
  }
  
  configuration_printer
  {
    class "csv_printer"
    parameters
    {
      delimiter ";"
      file STDOUT
    }
  } 
  stacked_view
  {
  }
}

initial_configuration
{
  class "cubic_crystal"
  parameters
  {
    dimension 3
    radius 2
    spacing 1
  }
  default_attributes
  {
    birth
    {
      efficiency 0.1
      resistance 0.5
    }
    lifespan
    {
      efficiency 0.1
      resistance 0.5
    }
    success
    {
      efficiency 0.9
      resistance 0.5
    }
    mutation
    {
      id 1
      parent
      {
        id 0
      }
    }
    dummy 0.1
  }
}

replay
{
  reader
  {
    class csv_reader
    parameters
    {
      file STDIN
      delimiter ";"
    }
  }
  
  key_attribute position
  
  nonkey_attributes
  {
    "" mutation.id
  }
  
}




