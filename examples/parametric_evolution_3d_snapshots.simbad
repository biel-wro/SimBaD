        {
          class "uniform_step_mutator"
          parameters
          {
            increase_length 0.1
            decrease_length 1.0
          }
        }
      }        
    }
    lifespan
    {
      saturation
      {
        sigma 10
        gamma 2
        scale 1000
      }
      efficiency
      {
        mutator
        {
          class "uniform_step_mutator"
          parameters
          {
            increase_length 0.1
            decrease_length 1.0
          }
        }
      }
      resistance
      {
        mutator
        {
          class "uniform_step_mutator"
          parameters
          {
            increase_length 0.1
            decrease_length 1.0
          }
        }
      }
    }
    success
    {
      saturation
      {
        sigma 5
        gamma 2
        scale 1
      }
      efficiency
      {
        mutator
        {
          class "uniform_step_mutator"
          parameters
          {
            increase_length 0.1
            decrease_length 1.0
          }
        }
      }
      resistance
      {
        mutator
        {
          class "uniform_step_mutator"
          parameters
          {
            increase_length 0.1
            decrease_length 1.0
          }
        }
      }
    }
    dummy
    {
      mutator
      {
        class "uniform_step_mutator"
        parameters
        {
          increase_length 0.1
          decrease_length 1.0
        }
      }
    }
  }
}




